    <!-- Start Small Banner  -->
	<section class="small-banner section">
        <div class="text-center ">
            <h2>SẢN PHẨM MỚI</h2>
        </div>
		<div class="container-fluid mt-4">
			<div class="row">
                <% if (newProducts) { 
                    newProducts.forEach( (product) => { %>
                        <!-- Single Banner  -->
				<div class="col-lg-4 col-md-6 col-12">
					<div style="position: relative; border: 1px solid #ccc; box-shadow: 0 4px 6px rgba(0, 0, 0, 0.3); transition: box-shadow 0.3s ease;" class="single-banner border">
                        <span style="padding: 5px 10px; background-color: #8493ca; border-radius: 10px; float: right; position: absolute; top: 0; right: 0;" class="new">New</span>
						<img src="/images/products/<%- product.image %>" alt="image">
						<div class="content text-center pl-0">
							<h3 class=" bg-light truncate"><%- product.name %></h3>
							<a class="bg-warning p-2" href="/product/<%- product._id %>">Xem ngay</a>
						</div>
					</div>
				</div>
				<!-- /End Single Banner  -->
                <%    })
                }%>
			</div>
		</div>
	</section>
